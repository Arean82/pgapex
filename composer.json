{
    "name": "pgapex/pgapex",
    "version": "1.0.0",
    "description": "PostgreSQL based web development environment driven by meta-data.",
    "keywords": ["PostgreSQL","PHP"],
    "homepage": "https://github.com/raitraidma/pgapex",
    "license": "MIT",
    "authors": [
        {
            "name": "Rait Raidma"
        }
    ],
    "config": {
        "vendor-dir": "pgapex/vendor"
    },
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "pgapex/app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "pgapex/tests/php/TestCase.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "mockery/mockery": "0.9.*"
    },
    "scripts": {
        "test": "php pgapex/vendor/bin/phpunit"
    }
}